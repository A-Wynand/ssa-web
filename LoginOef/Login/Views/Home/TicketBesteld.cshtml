@model IEnumerable<Festival.Models.DagBestelling>

@{
    ViewBag.Title = "TicketBesteld";
}

<h2>TicketBesteld</h2>

<p>You ordered this stuff:</p>

<table>
        <tr>
            <th>
                Soort
            </th>
            <th>
                Aantal
           </th>
        </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.soort)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.aantal)
            </td>
        </tr>

    }

</table>
